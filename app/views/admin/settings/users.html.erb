<div class="container">
  <%= render 'menu' %>
</div>
<div class="container-fluid">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Full Name</th>
        <th>Email</th>
        <th>Phone Number</th>
        <th>Total Hours</th>
        <th>Rank</th>
        <th>Deactivated?</th>
      </tr>
    </thead>
    <tbody>
      <% cache @users do %>
        <%= render partial: 'user', collection: @users %>
      <% end %>
    </tbody>
  </table>
</div>
